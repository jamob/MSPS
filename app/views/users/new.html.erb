<% provide(:title, 'Create user') %>
<h1>Create User</h1>

<div class="row">
  <div class="span5 offset2">
    <%= form_for(@user) do |f| %>
      <%= render 'form', f: f %>
      <div class="offset1">
        <%= f.submit "Create a user", class: "btn btn-large btn-primary" %>
        </div>
    <% end %>
  </div>
</div>
